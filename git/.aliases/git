alias gaa='git add -A'
alias gup='git up'
alias push='git push'
alias pull='git pull -p'
alias gm='git commit -m'
alias gma='git commit -am'
alias gd='git diff'
alias gdt='git difftool'
alias gdc='git diff --cached'
alias gs='git status'
alias gg='git gui &'
alias gk='gitk --all &'
alias gamend='git commit --amend --no-edit'
alias grewind='git reset --soft HEAD~1'
alias gclean='git clean -xfd'
alias gcleanall='find . -mindepth 1 -maxdepth 1 -type d -print -exec git -C {} clean -xfd \;'
alias stashpull='git stash && git pull && git stash pop'
alias pullall='find . -mindepth 1 -maxdepth 1 -type d -print -exec git -C {} pull \;'
alias pa='pullall'
alias others='git ls-files --others'
alias gcall='find . -mindepth 1 -maxdepth 1 -type d -print -exec git -C {} gc \;'
alias comall='find . -mindepth 1 -maxdepth 1 -type d -print -exec git -C {} co main \;'
